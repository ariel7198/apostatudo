datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Nivel {
  id              Int            @id @default(autoincrement())
  nivel           String
  profissionais  Profissional[]
}

model Profissional {
  id              Int      @id @default(autoincrement())
  nome            String
  sexo            String
  data_nascimento DateTime
  hobby           String
  nivelId         Int
  nivel           Nivel    @relation(fields: [nivelId], references: [id])
}
